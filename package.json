{
	"name": "createdirindex",
	"displayName": "Create Dir Index",
	"description": "create index.ts file in the dir.",
	"version": "0.0.2",
	"publisher": "saber2pr",
	"engines": {
		"vscode": "^1.56.0"
	},
	"icon": "assets/logo.png",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Saber2pr/vsc-ext-createdirindex.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:createdirindex"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "createdirindex",
				"title": "create index.ts file",
				"icon": "assets/logo.png"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "createdirindex",
					"group": "1_modification",
					"when": "explorerResourceIsFolder"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"release": "standard-version"
	},
	"devDependencies": {
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"standard-version": "^9.3.2",
		"typescript": "^4.1.3"
	}
}